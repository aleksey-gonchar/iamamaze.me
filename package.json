{
  "name": "emmofret",
  "version": "1.0.0",
  "description": "express.js + mongoose + mocha/chai + flux + react.js + TWBP boilerplate",
  "main": "server.js",
  "scripts": {
    "init": "node ./lib/initializers/secrets.js",
    "start": "node --harmony server.js",
    "start-prod": "NODE_ENV=production node --harmony server.js",
    "test": "npm run standard --verbose && npm run mocha",
    "mocha": "node --harmony ./node_modules/mocha/bin/_mocha --ui bdd",
    "standard": "node ./node_modules/standard/bin/cmd.js",
    "watch": "NODE_ENV=development node ./node_modules/webpack/bin/webpack --watch",
    "build": "node ./node_modules/webpack/bin/webpack",
    "build-prod": "NODE_ENV=production node ./node_modules/webpack/bin/webpack"
  },
  "keywords": [
    "boilerplate", "express", "react", "redux", "REST", "CRUD", "universal", "JWT"
  ],
  "author": "Tuiteraz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:aleksey-gonchar/emmofret.git"
  },
